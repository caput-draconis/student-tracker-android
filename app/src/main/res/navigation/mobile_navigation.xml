<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.studentvaccine.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_add_student"
        android:name="com.example.studentvaccine.ui.addstudent.AddStudentFragment"
        android:label="Add Student"
        tools:layout="@layout/fragment_add_student" />

    <fragment
        android:id="@+id/navigation_view_delete"
        android:name="com.example.studentvaccine.ui.viewdelete.ViewDeleteFragment"
        android:label="View/ Delete Student"
        tools:layout="@layout/fragment_view_delete" />

    <fragment
        android:id="@+id/navigation_manage_vaccine"
        android:name="com.example.studentvaccine.ui.managevaccine.ManageVaccine"
        android:label="Manage Vaccine"
        tools:layout="@layout/manage_vaccine_fragment" />
</navigation>